  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "npx mocha --recursive './test/**/*.js' --require @babel/register --require esm",
    "build": "npx spago bundle-module --main Helper --to src/utilities/_Helper.js && NODE_ENV=production npx rollup -c;",
